# change to home directory and source screenrc from ratpoison directory
chdir
source .ratpoison/screenrc

screen -t Admin0 0 
screen -t Admin1 1 
screen -t Admin2 2
screen -t Admin3 3
screen -t Admin4 4
screen -t Root0 5
select 5
exec sudo su
screen -t Root1 6
select 6
exec sudo su
screen -t Root2 7
select 7
exec sudo su

select 0
